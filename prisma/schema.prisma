generator client {
    provider = "prisma-client-js"
    previewFeatures = ["fullTextSearch"]
}

datasource db {
    provider = "postgresql"
    url      = env("POSTGRES_PRISMA_URL")
}

model tree {
    id             String @id @default(uuid())
    number         Int
    scientificName String
    commonName     String
    range          Int
    dap            Int
    meters         Int
    volumeM3       Int
}
